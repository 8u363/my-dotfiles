//  Thanks to Bina
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small",
        "padding": {
            "top": 2
        }
    },
    "display": {
        "separator": " ➜  "
    },
    "modules": [
        "break",
        "break",
        {
            "type": "os",
            "key": "OS   ",
            "keyColor": "31", // = color1
        },
        {
            "type": "kernel",
            "key": "   Kernel",
            "keyColor": "31",
            "format": "{2} {4}"
        },
        {
            "type": "packages",
            "key": "  󰏖 Packages",
            "keyColor": "31",
        },
        {
            "type": "uptime",
            "key": "   Uptime",
            "keyColor": "31",
        },
        {
            "type": "localip",
            "key": "  󰩟 LocalIP",
            "keyColor": "31",
            "showIpv4": true,
        },
        "break",
        {
            "type": "host",
            "format": "{5} {1} Type {2}",
            "key": "PC   ",
            "keyColor": "33",
        },
        {
            "type": "cpu",
            "format": "{1} ({3}) @ {7} GHz",
            "key": " ├  ",
            "keyColor": "33",
        },
        {
            "type": "memory",
            "key": " ├  ",
            "keyColor": "33",
        },
        {
            "type": "swap",
            "key": " ├ 󰓡 ",
            "keyColor": "33",
        },
        {
            "type": "disk",
            "key": " ├ 󰋊 ",
            "keyColor": "33",
        },
        "break",
        "break",
    ]
}